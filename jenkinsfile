pipeline {
    agent any

    stages {


        stage('Compile') {
            steps {
                bat 'javac Test.java'
            }
        }

        stage('Run') {
            steps {
                bat 'java Test'
            }
        }
    }

    post {
        success {
            echo 'Build Successful!'
        }
        failure {
            echo 'Build Failed!'
        }
    }
}